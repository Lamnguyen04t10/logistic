{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "EnableRequestLogging": true,
  "ConnectionStrings": {
    "Application": "Host=localhost;Port=5432;Username=user;Password=password;Database=jimshopdb"
  },
  "Configuration": {
    "Service": {
      "Host": "consul",
      "ConsulHost": "http://consul:8500",
      "Id": "system-api",
      "Name": "system-api",
      "Address": "system-api",
      "Port": 8080,
      "Tags": [
        "api",
        "system-api"
      ]
    },
    "Jwt": {
      "Audience": "my-api-audience",
      "Issuer": "my-api-issuer",
      "Secret": "this_is_a_secret_key_that_is_32_bytes_long_and_256_bits",
      "ExpireMinutes": 60
    },
    "Redis": {
      "Configuration": "redis:6379",
      "InstanceName": "Auth:"
    }
  }
}